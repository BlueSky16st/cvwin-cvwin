<?xml version="1.0"?>
<package >
  <metadata>
    <id>OpenCV.Win.ML</id>
    <version>310.6.0</version>
    <title>OpenCV Win ML (UWP)</title>
    <authors>OpenCV authors</authors>
    <owners>OpenCV authors</owners>
    <licenseUrl>https://github.com/cvwin/cvwin/blob/master/LICENSE</licenseUrl>
    <projectUrl>https://github.com/cvwin/cvwin</projectUrl>
    <iconUrl>https://raw.githubusercontent.com/cvwin/cvwin/master/opencv.win.png</iconUrl>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>Pre-built OpenCV binaries on Windows (UWP). This package contains native builds of OpenCV3 - C++ (x86 and x64). Note that VideoIO, and all dependent modules, require UWP Desktop extension.</description>
    <releaseNotes>Initial release of OpenCV 3.1.0 nuget package on UWP.</releaseNotes>
    <copyright>Copyright 2016, OpenCV, Harry Y</copyright>
    <tags>OpenCV, CVWin, Computer Vision, Cpp, C++, VS2015, visual studio, windows, universal windows, UWP, native, Win32</tags>
    <dependencies>
      <dependency id="OpenCV.Win.Core" version="310.6.0" />
    </dependencies>
  </metadata>
  <files>
<!--
    <file src="..\..\opencv-build-x64\install\include\opencv\*.h" target="include\opencv"/>
    <file src="..\..\opencv-build-x64\install\include\opencv\*.hpp" target="include\opencv"/>
-->
    <file src="..\..\opencv-build-x64\install\include\opencv2\ml.hpp" target="include\opencv2"/>
    <file src="..\..\opencv-build-x64\install\include\opencv2\ml\*.h" target="include\opencv2\ml"/>
    <file src="..\..\opencv-build-x64\install\include\opencv2\ml\*.hpp" target="include\opencv2\ml"/>

    <file src="..\..\opencv-build-x86\install\x86\vc14\lib\opencv_ml310.lib" target="build\native\win10-x86"/>
    <file src="..\..\opencv-build-x86\install\x86\vc14\lib\opencv_ml310d.lib" target="build\native\win10-x86"/>
    <file src="..\..\opencv-build-x64\install\x64\vc14\lib\opencv_ml310.lib" target="build\native\win10-x64"/>
    <file src="..\..\opencv-build-x64\install\x64\vc14\lib\opencv_ml310d.lib" target="build\native\win10-x64"/>

    <file src="..\..\opencv-build-x86\install\x86\vc14\bin\opencv_ml310.dll" target="runtimes\win10-x86\native"/>
    <file src="..\..\opencv-build-x86\install\x86\vc14\bin\opencv_ml310d.dll" target="runtimes\win10-x86\native"/>
    <file src="..\..\opencv-build-x64\install\x64\vc14\bin\opencv_ml310.dll" target="runtimes\win10-x64\native"/>
    <file src="..\..\opencv-build-x64\install\x64\vc14\bin\opencv_ml310d.dll" target="runtimes\win10-x64\native"/>

    <!--Adding .targets-->
    <file src="opencv.win.ml.targets" target="build\native"/>

  </files>
</package>