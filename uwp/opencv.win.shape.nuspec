<?xml version="1.0"?>
<package >
  <metadata>
    <id>OpenCV.Win.Shape</id>
    <version>310.5.7-dev</version>
    <title>OpenCV Win Shape (UWP)</title>
    <authors>OpenCV authors</authors>
    <owners>OpenCV authors</owners>
    <licenseUrl>https://github.com/cvwin/cvwin/blob/master/LICENSE</licenseUrl>
    <projectUrl>https://github.com/cvwin/cvwin</projectUrl>
    <iconUrl>https://raw.githubusercontent.com/cvwin/cvwin/master/opencv.win.png</iconUrl>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>Pre-built OpenCV binaries on Windows (UWP). This package contains native builds of OpenCV3 - C++ for Visual Studio 2015. (Supports x86 and x64.)</description>
    <releaseNotes>Initial release of OpenCV 3.1.0 nuget package on UWP.</releaseNotes>
    <copyright>Copyright 2016, OpenCV, Harry Y</copyright>
    <tags>OpenCV, CVWin, Computer Vision, Cpp, C++, VS2015, visual studio, windows, UWP, native, Win32</tags>
    <dependencies>
      <dependency id="OpenCV.Win.Core" version="310.5.7-dev" />
      <dependency id="OpenCV.Win.ImgProc" version="310.5.7-dev" />
      <dependency id="OpenCV.Win.Video" version="310.5.7-dev" />
    </dependencies>
  </metadata>
  <files>
<!--
    <file src="..\..\opencv-build-x64\install\include\opencv\*.h" target="include\opencv"/>
    <file src="..\..\opencv-build-x64\install\include\opencv\*.hpp" target="include\opencv"/>
-->
    <file src="..\..\opencv-build-x64\install\include\opencv2\shape.hpp" target="include\opencv2"/>
    <file src="..\..\opencv-build-x64\install\include\opencv2\shape\*.h" target="include\opencv2\shape"/>
    <file src="..\..\opencv-build-x64\install\include\opencv2\shape\*.hpp" target="include\opencv2\shape"/>

    <file src="..\..\opencv-build-x86\install\x86\vc14\lib\opencv_shape310.lib" target="build\native\win10-x86"/>
    <file src="..\..\opencv-build-x86\install\x86\vc14\lib\opencv_shape310d.lib" target="build\native\win10-x86"/>
    <file src="..\..\opencv-build-x64\install\x64\vc14\lib\opencv_shape310.lib" target="build\native\win10-x64"/>
    <file src="..\..\opencv-build-x64\install\x64\vc14\lib\opencv_shape310d.lib" target="build\native\win10-x64"/>

    <file src="..\..\opencv-build-x86\install\x86\vc14\bin\opencv_shape310.dll" target="runtimes\win10-x86\native"/>
    <file src="..\..\opencv-build-x86\install\x86\vc14\bin\opencv_shape310d.dll" target="runtimes\win10-x86\native"/>
    <file src="..\..\opencv-build-x64\install\x64\vc14\bin\opencv_shape310.dll" target="runtimes\win10-x64\native"/>
    <file src="..\..\opencv-build-x64\install\x64\vc14\bin\opencv_shape310d.dll" target="runtimes\win10-x64\native"/>

    <!--Adding .targets-->
    <file src="opencv.win.shape.targets" target="build\native"/>

  </files>
</package>